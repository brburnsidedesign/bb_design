{
  "parser": "babel-eslint",
  "rules": {
    "strict": 0,
    "no-whitespace-before-property": 2,
    "no-trailing-spaces": 2,
    "no-underscore-dangle": 0,
    "no-unused-vars": 1,
    "no-unused-expressions": ["error", { "allowShortCircuit": true }],
    "curly": 2,
    "no-multi-spaces": 1,
    "key-spacing": 2,
    "no-return-assign": 0,
    "consistent-return": 0,
    "no-shadow": 1,
    "no-comma-dangle": 0,
    "no-use-before-define": 1,
    "no-empty": 0,
    "new-parens": 2,
    "no-cond-assign": 1,
    "quotes": [2, "single", "avoid-escape"],
    "camelcase": 0,
    "new-cap": [1, { "capIsNew": false }],
    "no-undef": 1,
    "indent": [2, 4, {"SwitchCase": 1}],
    "prefer-const": "error",
    "semi": "error",
    "arrow-spacing": "error",
    "jasmine/no-focused-tests": "error",
    "jasmine/no-disabled-tests": "error"
  },
  "env": {
    "node": true,
    "browser": true,
    "jasmine": true,
    "es6": true,
    "jquery" : true
  },
  "globals": {
    "chrome": false,
    "ga": false,
    "ko": false,
    "browser": false,
    "__TESTING__": false
  }
}
